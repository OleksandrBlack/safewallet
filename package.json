{
  "name": "safewallet-app",
  "productName": "SafeWallet",
  "version": "0.0.35",
  "description": "SafeWallet Desktop App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "make-patch": "./make-patch.sh",
    "make-rpm": "node make-rpm.js",
    "make-deb": "node make-deb.js",
    "dist": "rm -r dist/* && build -wl",
    "dist-lin-deb": "sh binary_lin.sh && build -l --config=buildscripts/debbuild.yaml && mkdir -p ~/wallets/ && cp dist/*.deb ~/wallets/ && rm -r dist/*",
    "dist-lin-tar": "sh binary_lin.sh && build -l --config=buildscripts/tarbuild.yaml && mkdir -p ~/wallets/ && cp dist/*.gz ~/wallets/ && rm -r dist/*",
    "dist-win-exe": "sh binary_win.sh && build -w --config=buildscripts/exebuild.yaml && mkdir -p ~/wallets/ && cp dist/*.exe ~/wallets/ && rm -r dist/*",
    "dist-win-zip": "sh binary_win.sh && build -w --config=buildscripts/zipbuild.yaml && mkdir -p ~/wallets/ && cp dist/*.zip ~/wallets/ && rm -r dist/*",
    "dist-mac-zip": "sh binary_mac.sh && build -m --config=buildscripts/macbuild.yaml && mkdir -p ~/wallets/ && cp dist/*.zip ~/wallets/ && rm -r dist/*"
  },
  "repository": "https://github.com/fair-exchange/Safewallet/",
  "homepage": "https://safecoin.org/",
  "keywords": [
    "safewallet",
    "fair-exchange",
    "safecoin",
    "multicoin",
    "wallet",
    "spv",
    "jumblr"
  ],
  "author": "SafeCoin DEV Team <safe@safecoin.org>",
  "license": "MIT",
  "devDependencies": {
    "electron-builder": "20.28.4",
    "electron": "2.0.8"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "arch": "^2.1.0",
    "async": "^2.6.0",
    "bigi": "^1.4.2",
    "bip39": "^2.5.0",
    "bitcoinforkjs-lib": "git://github.com/Fair-Exchange/bitcoinforkjs-lib.git",
    "bitcoinjs-lib": "git://github.com/Fair-Exchange/bitcoinjs-lib.git",
    "bitcoinjs-lib-pos": "git://github.com/Fair-Exchange/bitcoinjs-lib-pos",
    "bitgo-utxo-lib": "git://github.com/Fair-Exchange/bitgo-utxo-lib.git",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "buffer-reverse": "^1.0.1",
    "coinselect": "github:bitcoinjs/coinselect",
    "electron-packager": "^12.0.1",
    "electrum": "^4.21.0",
    "express": "^4.14.0",
    "fix-path": "^2.1.0",
    "fs-extra": "^4.0.2",
    "graceful-fs": "^4.1.11",
    "iocane": "^1.0.0",
    "js-sha256": "^0.7.1",
    "lz-string": "^1.4.4",
    "marketmaker": "git://github.com/pbca26/marketmaker",
    "nodejs-aes256": "^1.0.1",
    "passwd-strength": "git+https://github.com/pbca26/passwd-strength.git",
    "portscanner": "^2.1.1",
    "ps-node": "^0.1.5",
    "remote-file-size": "^3.0.3",
    "request": "^2.80.0",
    "sha256": "^0.2.0",
    "socket.io": "^1.7.3",
    "tx-decoder": "git://github.com/pbca26/tx-decoder",
    "webpack": "^4.6.0",
    "wif": "^2.0.6",
    "cors-anywhere": "0.4.1"
  }
}
